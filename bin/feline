#!/usr/bin/env ruby

require "bundler/setup"
require "feline"

def input_handler(arg)
  case arg
    when "file"
      Feline::File.new.move
    when "fact"
      puts Feline::Fact.new.fact
    when "foo"
      puts "Do something else"
    else
      Feline::Browser.new.open
  end
end

input_handler(ARGV[0])
exit
